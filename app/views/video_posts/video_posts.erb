<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<h2>Lixer Feed</h2>

<% if logged_in? %>
<h3>Welcome, <%= @user.username %>
<br>
<a href="/logout">Log Out</a>
<br>
<a href="/video-posts/new">Create Post</a>
</h3>
<% else %>
<a href="/login">Log In</a>
<br>
<a href="/signup">Sign Up</a>
<% end %>
<ul>
  <% VideoPost.all.each do |videopost| %>
  <li><a href=""><%= videopost.title %></a></li>
  <% end %>
</ul>