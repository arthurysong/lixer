<h2><%= @user.username %>'s liked Video Posts</h2>
<ul>
  <% @user.liked_video_posts.each do |video_post| %>
  <li>
    <div class="video-post">
      <iframe width="200" height="130" src="<%= video_post.video_link %>">
      </iframe>
      <a href="/video-posts/<%= video_post.id %>"><%= video_post.title %></a>
    </div>
  </li>
  <% end %>
  
</ul>

<% if @user.liked_video_posts.empty? %>
  <p>You have not liked any videos.</p>
<% end %>

<div style="clear: both"></div>