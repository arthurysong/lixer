<h1><%= @user.username %>'s liked Video Posts</h1>
<ul>
  <% @user.liked_video_posts.each do |video_post| %>
  <li>
    <div class="video-post">
      <video id="player" width="320" height="180" controls>
        <source src="<%= video_post.video_url %>#t=0.5" type="video/mp4">
      </video>
      <a href="/video-posts/<%= video_post.id %>"><%= video_post.title %></a>
      <p id="date"><%= video_post.created_at.strftime('%b %e, %Y') %></p>
      <p><%= video_post.creator.username %></p>
    </div>
  </li>
  <% end %>
  
</ul>

<% if @user.liked_video_posts.empty? %>
  <p>You have not liked any videos.</p>
<% end %>

<div style="clear: both"></div>